{
  "name": "broccoli-clean-css",
  "version": "1.1.0",
  "description": "CSS minifier for Broccoli, using clean-css",
  "scripts": {
    "pretest": "eslint --fix --format=codeframe index.js test.js",
    "test": "nyc node --throw-deprecation --harmony_async_await test.js"
  },
  "repository": "shinnn/broccoli-clean-css",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "broccoli-plugin",
    "css",
    "cleancss",
    "clean-css",
    "minifier",
    "minification",
    "compress",
    "compressor",
    "postprocess",
    "optimization",
    "optimize",
    "optimizer",
    "whitespace"
  ],
  "dependencies": {
    "broccoli-persistent-filter": "^1.2.13",
    "clean-css-promise": "^2.0.0",
    "json-stable-stringify": "^1.0.1",
    "source-map-to-comment": "^1.1.0"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^3.0.0",
    "broccoli-fixture": "^1.0.0",
    "eslint": "^3.14.1",
    "lodash": "^4.17.4",
    "nyc": "^10.1.2",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "@shinnn/node",
    "rules": {
      "no-underscore-dangle": "off"
    }
  }
}
